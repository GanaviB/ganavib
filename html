    <script>
        //To Prevent Focus Lost on Post Back in Update Panel
        var postbackElement = null;
        function RestoreFocus(source, args) {
            if (postbackElement != null && postbackElement.id != null) {
                var PostBackControl = document.getElementById(postbackElement.id);

                if (PostBackControl != null && typeof (PostBackControl.type) !== 'undefined') {
                    var x = PostBackControl.type;

                    if (x == "select-one")
                        PostBackControl.focus();

                    if (PostBackControl.classList.contains("btnSave"))
                        GNScrollToTop();
                }
            }
        }
        function SavePostbackElement(source, args) {
            postbackElement = args.get_postBackElement();
        }
        function AddRequestHandler() {

            // if (typeof Sys !== 'undefined') {
            var prm = Sys.WebForms.PageRequestManager.getInstance();
            prm.add_endRequest(RestoreFocus);
            prm.add_beginRequest(SavePostbackElement);
            //  }
        }
    </script></head>
